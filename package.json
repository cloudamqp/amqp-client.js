{
  "type": "module",
  "name": "@cloudamqp/amqp-client",
  "version": "1.1.3",
  "description": "AMQP 0-9-1 client, both for browsers (WebSocket) and node (TCP Socket)",
  "main": "dist/amqp-socket-client.js",
  "types": "dist/typings/amqp-socket-client.d.ts",
  "scripts": {
    "docs": "typedoc",
    "test": "ava",
    "prebuild": "sed -i'' \"s/VERSION = .*/VERSION = '$npm_package_version'/\" src/amqp-base-client.ts && git add src/amqp-base-client.ts",
    "build": "tsc && rollup -c && prettier --write ./dist/amqp-websocket-client.mjs",
    "preversion": "npm test",
    "version": "npm run build && git add -A dist",
    "lint": "eslint . --ext .ts"
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nonSemVerExperiments": {
      "configurableModuleFormat": true
    },
    "nodeArguments": [
      "--loader=ts-node/esm",
      "--experimental-specifier-resolution=node"
    ],
    "verbose": true
  },
  "files": [
    "src/",
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cloudamqp/amqp-client.js.git"
  },
  "keywords": [
    "amqp",
    "rabbitmq"
  ],
  "author": "CloudAMQP <contact@cloudamqp.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/cloudamqp/amqp-client.js/issues"
  },
  "homepage": "https://github.com/cloudamqp/amqp-client.js#readme",
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.2.1",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "ava": "^3.15.0",
    "esdoc": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^7.20.0",
    "prettier": "^2.3.0",
    "rollup": "^2.39.1",
    "rollup-plugin-cleanup": "^3.2.1",
    "ts-node": "^10.0.0",
    "typedoc": "^0.20.36",
    "typescript": "^4.2.4"
  }
}
